x1=250
y1=250
x2=250
y2=250
x3=250
y3=250
x4=250
y4=250
def setup():
    size(500,500)
def draw():
    global x1,y1,x2,y2,x3,y3,x4,y4
    circle(x1,y1,50)
    circle(x2,y2,50)
    circle(x3,y3,50)
    circle(x4,y4,50)
    x1=x1+1
    y1=y1+1
    x2=x2-1
    y2=y2+1
    x3=x3+1
    y3=y3-1
    x4=x4-1
    y4=y4-1
