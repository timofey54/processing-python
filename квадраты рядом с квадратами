def re(x,y,z):
    if z>18:
        square(0,0,z)
        push()
        translate(-x,0)
        re(x/2,y/2,z*3/4)
        pop()
        push()
        translate(x,0)
        re(x/2,y/2,z*3/4)
        pop()
        push()
        translate(0,-y)
        re(y/2,x/2,z*3/4)
        pop()
        push()
        translate(0,y)
        re(y/2,x/2,z*3/4)
        pop()
def setup():
    size(500,500)
    rectMode(CENTER)
def draw():
    translate(250,250)
    re(120,120,50)
