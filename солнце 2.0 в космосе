x=0
def setup():
    size(500,500)
def draw():
    background("#030303")
    strokeWeight(0)
    fill("#DDFF03")
    translate(250,250)
    circle(0,0,50)
    global x
    rotate(radians(x))
    x=x+1
    triangle(-20,25,-3,40,15,25)
    rotate(radians(45))
    triangle(-20,25,-3,40,15,25)
    rotate(radians(45))
    triangle(-20,25,-3,40,15,25)
    rotate(radians(45))
    triangle(-20,25,-3,40,15,25)
    rotate(radians(45))
    triangle(-20,25,-3,40,15,25)
    rotate(radians(45))
    triangle(-20,25,-3,40,15,25)
    rotate(radians(45))
    triangle(-20,25,-3,40,15,25)
    rotate(radians(45))
    triangle(-20,25,-3,40,15,25)
    rotate(radians(45))
