def setup():
    size(500,500)
def draw():
    y=500
    y1=0
    x=500
    x1=0
    background(0)
    while mouseY<y:
        rect(mouseX,y,50,50)
        y=y-50
    while mouseY>y1:
        rect(mouseX,y1,50,50)
        y1=y1+50
    while mouseX<x:
        rect(x,mouseY,50,50)
        x=x-50
    while mouseX>x1:
        rect(x1,mouseY,50,50)
        x1=x1+50
